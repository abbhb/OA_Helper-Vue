{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "declaration": true,
    "strict": false,
    "skipLibCheck": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "target": "esnext",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "lib": [
      "dom",
      "ESNext",
      "DOM",
      "DOM.Iterable",
      "ScriptHost"
    ],

    "allowJs": true
  },
  "include": ["src/**/*", "src/**/*.vue","src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx","src/**/*.vue","components.d.ts","auto-imports.d.ts"],
  "exclude": [
    "node_modules",
    "dist",
    "public",
    "docs"
  ]
}
